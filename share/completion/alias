# completion settings for the alias builtin
# written by magicant
# vim: set ft=sh ts=8 sts=8 sw=8 noet:

complete -C "$1" -Xa
complete -C "$1" -O g -O --global \
	-D "define global aliases"
complete -C "$1" -O p -O --prefix \
	-D "print aliases as reusable shell commands"
complete -C "$1" -O --help
