# completion settings for the dirname command (POSIX; GNU coreutils 8.4)
# written by magicant
# vim: set ft=sh ts=8 sts=8 sw=8 noet:

case "$("$1" --version 2>/dev/null)" in
	*'coreutils'*)
		typeset gnu=true
		;;
	*)
		typeset gnu=
		;;
esac

complete -C "$1" -f
if [ "$gnu" ]; then
	complete -C "$1" -O --help
	complete -C "$1" -O --version
fi
